{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "chelout/laravel-relationship-events": "^1.5",
        "fileeye/mimemap": "^2.0",
        "fruitcake/laravel-cors": "^3.0.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hammerstone/fast-paginate": "^0.1.11",
        "http-interop/http-factory-guzzle": "^1.2",
        "jcupitt/vips": "2.1.0",
        "laravel/framework": "^9.52.0",
        "laravel/sanctum": "^3.0.1",
        "laravel/scout": "^9.6.2",
        "laravel/telescope": "^4.13.0",
        "laravel/tinker": "^2.7.3",
        "league/flysystem-aws-s3-v3": "^3.10",
        "rennokki/laravel-eloquent-query-cache": "^3.3.1",
        "socialiteproviders/discord": "^4.1",
        "socialiteproviders/github": "^4.1",
        "socialiteproviders/gitlab": "^4.1",
        "socialiteproviders/steam": "^4.1",
        "spatie/laravel-morph-map-generator": "^1.1.0",
        "spatie/laravel-query-builder": "^5.1.2",
        "spatie/laravel-validation-rules": "^3.2",
        "staudenmeir/eloquent-eager-limit": "^1.6"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "fakerphp/faker": "^1.21.0",
        "knuckleswtf/scribe": "^4.14.0",
        "laravel/sail": "^1.16.6",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.3.2",
        "phpunit/phpunit": "^9.5.27",
        "spatie/laravel-ignition": "^1.6.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
