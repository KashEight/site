{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "biiiiiigmonster/hasin": "^3.0",
        "chelout/laravel-relationship-events": "^2.0",
        "coderflex/laravel-turnstile": "^1.0",
        "fileeye/mimemap": "^2.0",
        "genealabs/laravel-model-caching": "^11.0.0",
        "ggsoftwarellc/nitropay-sponsor-php": "^2.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "hammerstone/fast-paginate": "^1.0.1",
        "http-interop/http-factory-guzzle": "^1.2",
        "illuminatech/multipart-middleware": "^1.1",
        "jcupitt/vips": "2.3.0",
        "laravel/framework": "^10.45.0",
        "laravel/octane": "^2.2.1",
        "laravel/pulse": "^1.0@beta",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.0.2",
        "laravel/telescope": "^4.13.0",
        "laravel/tinker": "^2.7.3",
        "lcobucci/jwt": "4.1",
        "laravel/socialite": "^5.12.1",
        "league/flysystem-aws-s3-v3": "^3.10",
        "mailersend/laravel-driver": "^2.2",
        "sentry/sentry-laravel": "^4.1",
        "socialiteproviders/discord": "^4.1",
        "socialiteproviders/github": "^4.1",
        "socialiteproviders/gitlab": "^4.1",
        "socialiteproviders/steam": "^4.1",
        "spatie/laravel-morph-map-generator": "^1.1.0",
        "spatie/laravel-query-builder": "^5.1.2",
        "spatie/laravel-validation-rules": "^3.2",
        "staudenmeir/eloquent-eager-limit": "^1.6",
        "ultrono/laravel-sitemap": "^9.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.21.0",
        "knuckleswtf/scribe": "^4.21.2",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
